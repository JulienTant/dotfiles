# Set default values for all following accounts.
defaults
auth           on
tls            on
tls_trust_file /etc/ssl/certs/ca-certificates.crt
logfile        ~/.local/state/msmtp/msmtp.log

account aosixphone@gmail.com
host smtp.gmail.com
port 587
from aosixphone@gmail.com
user aosixphone@gmail.com
passwordeval "pass emails/aosixphone@gmail.com"

account julien@craftyx.fr
host smtp.gmail.com
port 465
from julien@craftyx.fr
user julien@craftyx.fr
passwordeval "pass emails/julien@craftyx.fr"
